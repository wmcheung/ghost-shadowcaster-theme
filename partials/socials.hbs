        </a>
    </li>
    {{/if}}
    {{#if @custom.linkedin}}
    <li class="pl-4">
        <a href="{{@custom.linkedin}}" target="_blank" rel="noopener">
            LinkedIn
        </a>
    </li>
    {{/if}}
    {{#if @custom.keybase}}
    <li class="pl-4">
        <a href="{{@custom.keybase}}" target="_blank" rel="noopener">
            Keybase
        </a>
    </li>
