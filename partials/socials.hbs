<ul class="flex gap-4 divide-x">
    {{#if @custom.github}}
    <li>
        <a href="{{@custom.github}}" target="_blank" rel="noopener">
            GitHub
        </a>
    </li>
    {{/if}}
    {{#if @custom.twitter}}
    <li class="pl-4">
        <a href="{{@custom.twitter}}" target="_blank" rel="noopener">
